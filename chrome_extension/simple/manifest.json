{
  "name": "nisin url watcher",
  "version": "1.0",
  "manifest_version": 2,
  "description": "nisin - A Simple Web Difference Engine",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"contextMenus", "tabs", "http://*/*", "https://*/*"
  ]
}
