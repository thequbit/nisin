{
    "name": "WebWatcher Extension",
    "description": "Add URL to WebWatcher list",
    "version": "1.1",
    "background_page": "background.html",
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ],
    "browser_action": {
        "default_title": "Add URL to WebWatcher list",
        "default_icon": "icon.png",
        "popup": "popup.html"
    }
}